<tr class="exclusion-<%= exclusion.id %>">
  <td><%= link_to "", exclusion_path(exclusion.id), method: :delete, data: {confirm: "Are you sure you want to delete this exclusion?", remote: true}, class: "delete-icon" %></td>
  <td><%= exclusion.id %></td>
  <td><%= exclusion.exclusion_type %></td>
  <td><%= exclusion.exclusion_id %></td>
  <td><%= exclusion.updated_at.strftime("%m/%d/%Y %I:%M:%S %p") %></td>
</tr>
